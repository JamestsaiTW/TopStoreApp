<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="TopStoreApp.Pages.GoodsPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:viewmodels="clr-namespace:TopStoreApp.ViewModels"
    x:DataType="viewmodels:GoodsPageViewModel">

    <StackLayout>
        <CollectionView ItemSizingStrategy="MeasureFirstItem" ItemsSource="{Binding Goods}" />
    </StackLayout>

</ContentPage>